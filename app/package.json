{
  "name": "@relay-modern-boilerplate/app",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/enisdenjo/relay-modern-boilerplate.git"
  },
  "license": "MIT",
  "description": "React application for relay-modern-boilerplate",
  "scripts": {
    "relay:get-schema": "get-graphql-schema http://postgraphile:${POSTGRAPHILE_PORT}/graphql > schema.graphql",
    "relay:compile": "relay-compiler --src ./src --schema ./schema.graphql --language typescript --artifactDirectory ./src/artifacts",
    "relay:watch": "relay-compiler --src ./src --schema ./schema.graphql --language typescript --artifactDirectory ./src/artifacts --watch",
    "build:dll": "NODE_ENV=development webpack --config webpack.dll.config.js --hide-modules --display-chunks --color --progress",
    "dev:server": "NODE_ENV=development webpack-dev-server --config ./webpack.dev.config.js",
    "dev": "NODE_ENV=development concurrently -n relay-compiler,webpack-dev-server \"npm run relay:watch\" \"webpack-dev-server --config ./webpack.dev.config.js\""
  },
  "dll": {
    "name": "vendors",
    "path": "node_modules/.dll",
    "exclude": []
  },
  "dependencies": {
    "@material-ui/core": "3.2.0",
    "@material-ui/icons": "3.0.1",
    "lodash": "4.17.11",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-loadable": "5.5.0",
    "react-relay": "1.7.0-rc.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "relay-runtime": "1.7.0-rc.1"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@types/react": "16.4.16",
    "@types/react-dom": "16.0.8",
    "@types/react-loadable": "5.4.1",
    "@types/react-relay": "1.3.9",
    "@types/react-router-dom": "4.3.1",
    "@types/relay-runtime": "1.3.6",
    "add-asset-html-webpack-plugin": "3.0.1",
    "babel-loader": "8.0.4",
    "babel-plugin-relay": "1.7.0-rc.1",
    "clean-webpack-plugin": "0.1.19",
    "concurrently": "4.0.1",
    "css-loader": "1.0.0",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin": "0.4.10",
    "get-graphql-schema": "2.1.1",
    "graphql": "14.0.2",
    "graphql-compiler": "1.7.0-rc.1",
    "html-webpack-plugin": "3.2.0",
    "relay-compiler": "1.7.0-rc.1",
    "relay-compiler-language-typescript": "1.1.1",
    "style-loader": "0.23.1",
    "ts-loader": "5.2.1",
    "typescript": "3.1.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.9",
    "webpack-merge": "4.1.4"
  }
}
